# Definition for singly-linked list.
# class ListNode(object):
#     def __init__(self, x):
#         self.val = x
#         self.next = None

class Solution(object):
    def getIntersectionNode(self, headA, headB):
        """
        :type head1, head1: ListNode
        :rtype: ListNode
        """
        if headA and headB:
            A = {headA}
            
            while headA.next:
                headA = headA.next
                A.add(headA)  
            while headB:
                if headB in A:
                    return headB
                else:
                    headB = headB.next
        
